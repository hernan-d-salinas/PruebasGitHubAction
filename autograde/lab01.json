{
  "lab": "lab01",
  "summary_var": "LAB_SUMMARY",

  "required_keys": [
    "student",
    "clean_shape",
    "sp1",
    "sp2",
    "alpha",
    "test",
    "pvalue",
    "effect",
    "bootstrap_ci_diff",
    "dashboard"
  ],

  "rules": {
    "pvalue_range": [0.0, 1.0],
